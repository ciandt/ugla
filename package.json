{
  "name": "ugla",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --project=ugla",
    "sass-lint": "node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "lint": "npx ng run ugla:eslint && npm run sass-lint",
    "e2e": "ng e2e",
    "deploy:site": "ng build && firebase deploy",
    "build:all": "npm run build-lib:ugla && npm run build-lib:ugla-rules",
    "build-lib:ugla": "node -e 'require(\"./version\").changeConfig()' && ng-packagr -p projects/ugla/package.json && cp -r projects/ugla/src/sass/ dist/ugla/sass && cd dist/ugla && npm pack",
    "build-lib:ugla-rules": "ng-packagr -p projects/ugla-rules/package.json && cd dist/ugla-rules && npm pack",
    "postinstall": "ngcc",
    "docs": "node_modules/typedoc/bin/typedoc --plugin typedoc-loopingz-theme"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ciandt/ugla.git"
  },
  "bugs": {
    "url": "https://github.com/ciandt/ugla/issues"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "keywords": [
    "angular",
    "javascript",
    "typescript",
    "components",
    "library",
    "scss"
  ],
  "homepage": "https://ugla.dev",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/architect": "^0.1100.6",
    "@angular/animations": "^11.0.8",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/forms": "^11.0.8",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "@angular/service-worker": "^11.0.8",
    "core-js": "^2.6.11",
    "filepond": "^4.10.0",
    "filepond-plugin-file-validate-size": "^2.2.0",
    "filepond-plugin-file-validate-type": "^1.2.2",
    "filepond-plugin-image-crop": "^2.0.1",
    "filepond-plugin-image-exif-orientation": "^1.0.4",
    "filepond-plugin-image-preview": "^4.6.1",
    "filepond-plugin-image-resize": "^2.0.2",
    "filepond-plugin-image-transform": "^3.7.1",
    "filepond-plugin-image-validate-size": "^1.2.4",
    "js-datepicker": "^4.6.6",
    "moment": "^2.24.0",
    "ngx-filepond": "^5.0.1",
    "react-navigation-stack": "^2.1.1",
    "rxjs": "~6.6.3",
    "tooltip.js": "^1.3.3",
    "tslib": "^2.0.0",
    "typescript": "^4.0.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular-devkit/core": "^11.0.6",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/schematics": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "^11.0.6",
    "@angular/compiler-cli": "^11.0.8",
    "@angular/language-service": "^11.0.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "serialize-javascript": "^3.1.0",
    "set-value": ">=2.0.1",
    "tar": "^4.4.13",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.2",
    "typedoc": "^0.20.28",
    "typedoc-clarity-theme": "^1.1.0",
    "typedoc-dark-theme": "0.0.11",
    "typedoc-loopingz-theme": "^1.1.0"
  },
  "main": "dist/ugla/ugla.d.ts",
  "directories": {
    "lib": "dist/*"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=10.0.0"
  }
}
